{
  "name": "@atom/teletype-server",
  "version": "0.18.2",
  "engines": {
    "node": "16.16.0",
    "npm": "8.11.0"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "migrate": "pg-migrate",
    "test": "mocha test/setup.js **/*.test.js --ui=tdd",
    "test:middleware": "mocha test/setup.js test/middleware.test.js --ui=tdd",
    "test:identity": "mocha test/setup.js test/identity-provider.test.js --ui=tdd",
    "test:controller": "jest --selectProjects Controller",
    "start": "node script/server",
    "migrations": "pg-migrations apply --directory ./migrations",
    "start:dev": "cross-env PULSAR_STATUS=dev node script/server"
  },
  "author": "",
  "license": "MIT",
  "pg": {
    "test": {
      "migrationScript": [
        "npm",
        "run",
        "migrations"
      ],
      "image": "postgres:14.5-alpine"
    }
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-pg-migrate": "^2.25.0",
    "pg-promise": "^8.1.1",
    "postgres": "^3.3.3",
    "pusher": "^2.0.1",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "server-destroy": "^1.0.1",
    "temp": "^0.8.3",
    "throng": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@databases/pg-migrations": "^5.0.2",
    "@databases/pg-test": "^3.1.2",
    "deep-equal": "^1.0.1",
    "mocha": "^5.2.0"
  }
}
